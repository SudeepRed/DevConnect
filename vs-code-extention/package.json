{
  "name": "devconnect",
  "displayName": "DevConnect",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devconnect.helloWorld",
    "onCommand:devconnect.authenticate"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "devconnect.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "devconnect.askQuestion",
        "category": "devconnect",
        "title": "Ask Question"
      },
      {
        "command": "devconnect.refresh",
        "category": "devconnect",
        "title": "Refresh"
      },
      {
        "command": "devconnect.addTodo",
        "category": "devconnect",
        "title": "Add Todo From Selection"
      },
      {
        "command": "devconnect.authenticate",
        "category": "devconnect",
        "title": "Authenticate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch \"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rgossiaux/svelte-headlessui": "^1.0.0-beta.12",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/polka": "^0.5.4",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-electron": "^2.1.2",
    "axios": "^0.26.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.9.0",
    "express": "^4.17.3",
    "glob": "^7.2.0",
    "mocha": "^9.2.1",
    "rollup": "^2.70.1",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.23",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "twind": "^0.16.16",
    "typescript": "^4.6.3",
    "utilcomps": "^1.10.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "polka": "^0.5.2"
  }
}
